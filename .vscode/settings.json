{
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
    },
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
    },
    "eslint.validate": [
        "javascript",
        "typescript",
        "toml",
        "json",
        "markdown",
    ],
    "cSpell.words": [
        "indoc",
        "instanceof",
        "napi",
        "wasmup",
    ],
}
